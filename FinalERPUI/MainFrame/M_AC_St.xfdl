        <Static id="lb_2" taborder="7" text="2" left="768" top="630" width="40" height="40" font="bold 20px/normal &quot;나눔바른펜&quot;" textAlign="center" border="1px solid #999999" verticalAlign="middle" cursor="pointer"/>
        <Static id="lb_3" taborder="8" text="3" left="833" top="630" width="40" height="40" font="bold 20px/normal &quot;나눔바른펜&quot;" textAlign="center" border="1px solid #999999" verticalAlign="middle" cursor="pointer"/>
        <Static id="lb_4" taborder="9" text="4" left="898" top="630" width="40" height="40" font="bold 20px/normal &quot;나눔바른펜&quot;" textAlign="center" border="1px solid #999999" verticalAlign="middle" cursor="pointer"/>
        <Static id="lb_5" taborder="10" text="5" left="963" top="630" width="40" height="40" font="bold 20px/normal &quot;나눔바른펜&quot;" textAlign="center" border="1px solid #999999" verticalAlign="middle" cursor="pointer"/>
        <Static id="lb_right" taborder="11" text="&gt;" left="1028" top="630" width="40" height="40" font="bold 20px/normal &quot;나눔바른펜&quot;" textAlign="center" border="1px solid #999999" verticalAlign="middle" cursor="pointer"/>
        <Button id="btn_apply" taborder="12" text="신청" left="1440" top="631" width="80" height="40" border="1px solid #c7c7c7" background="#ffffff" color="black" font="18px/normal &quot;나눔바른펜&quot;&#9;" onclick="btn_apply_onclick"/>
      </Layout>
    </Layouts>
    <Script type="xscript5.1"><![CDATA[include "lib::libCommon.xjs";
include "lib::lib_date.xjs";
include "lib::lib_base.xjs";

this.application = null;

this.M_AC_St_onload = function(obj:nexacro.Form,e:nexacro.LoadEventInfo)
{
   this.application = nexacro.getApplication();
   /*회계 신청현황(전체 조회) 함수 호출*/
   this.mac_board_load();
};


/*회계 신청현황(전체 조회)*/
this.mac_board_load = function(){
   var objgdsLoginInfo = this.application.objgdsLoginInfo;
   var macNparam = " e_no=" + objgdsLoginInfo.getColumn(0,"E_NO") + " d_name=회계과";
   var idCount = this.application.transactionCount++;
   var svcID = "getAllSel" + idCount;
   this.ds_my_ap_ac.clearData();
   
   this.transaction(svcID
   ,"SvcURL::/macst/getAllSel.mpg?no=" + idCount
   ,""
   ,"ds_my_ap_ac=out_test"
   ,macNparam
   ,"");
};

/*신청 버튼*/
this.btn_apply_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
   /*회계 신청 모달 출력*/
   this.application.gfnShowModal("popupModal", null, "ModalFrame::M_AC_Type.xfdl", "", this, 550, 30, true, true, null);
};



/*회계 신청현황(조회 버튼 선택 시 함수)*/
this.macSel_load = function(){
   var objgdsLoginInfo = this.application.objgdsLoginInfo;
   var selectedCombo = this.cb_type.text;
   if(selectedCombo === "신청서 종류"){
   var macSelparam = " e_no=" + objgdsLoginInfo.getColumn(0,"E_NO") + " d_name=회계과";
   }
   else if(selectedCombo === "전자결재"){
		selectedCombo += "_회계";
		var macSelparam = " e_no=" + objgdsLoginInfo.getColumn(0,"E_NO") + " d_name=회계과" + " ft_type=" + selectedCombo;
   }
//    else if(selectedCombo === "계좌정보"){
// 		/*selectedCombo = "계좌\u00a0정보\u00a0변경";*/
// 		var macSelparam = " e_no=" + objgdsLoginInfo.getColumn(0,"E_NO") + " d_name=회계과" + " ft_type=" + selectedCombo;
//    }
   
   else{
		var macSelparam = " e_no=" + objgdsLoginInfo.getColumn(0,"E_NO") + " d_name=회계과" + " ft_type=" + selectedCombo;
   }
   /*var mhrSelparam = " e_no=" + objgdsLoginInfo.getColumn(0,"E_NO") + "ft_type=" + objgdsLoginInfo.getColumn(0,"FT_TYPE");*/
   var idCount = this.application.transactionCount++;
   var svcID = "macSelType" + idCount;
   this.ds_my_ap_ac.clearData();
   
   this.transaction(svcID
   ,"SvcURL::/macst/getAllSel.mpg?no=" + idCount
   ,""
   ,"ds_my_ap_ac=out_test"
   ,macSelparam
   ,"");
};

this.btn_sel_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
   alert("조회를 시작합니다.");
   /*인사 신청현황(전체 조회) 함수 호출*/
   this.macSel_load();
};
]]></Script>
    <Objects>
